{
	"name": "draw-to-escape",
	"globIgnorePaths": ["**/*.spec.luau"],
	"tree": {
		"$className": "DataModel",
		"ReplicatedStorage": {
			"Shared": {
				"$path": "src/shared"
			}
		},
		"ServerScriptService": {
			"Server": {
				"$path": "src/server"
			}
		},
		"StarterPlayer": {
			"StarterPlayerScripts": {
				"Client": {
					"$path": "src/client"
				}
			}
		},
		"Workspace": {
			"$properties": {
				"Gravity": 196.2
			}
		},
		"Lighting": {
			"$className": "Lighting",
			"$properties": {
				"Technology": "Future",
				"GlobalShadows": true,
				"Brightness": 2,
				"ClockTime": 14,
				"Ambient": [0.3, 0.3, 0.35],
				"OutdoorAmbient": [0.4, 0.4, 0.45]
			},
			"Atmosphere": {
				"$className": "Atmosphere",
				"$properties": {
					"Density": 0.2,
					"Offset": 0.25,
					"Color": [0.9, 0.9, 0.95],
					"Decay": [0.85, 0.85, 0.9],
					"Glare": 0,
					"Haze": 1
				}
			},
			"Bloom": {
				"$className": "BloomEffect",
				"$properties": {
					"Intensity": 0.3,
					"Size": 24,
					"Threshold": 0.8
				}
			}
		},
		"SoundService": {
			"$className": "SoundService"
		},
		"StarterGui": {
			"$className": "StarterGui",
			"$properties": {
				"ScreenOrientation": "LandscapeSensor"
			}
		}
	}
}
